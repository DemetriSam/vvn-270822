includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:
  level: 9
  paths:
    - app
    - database
    - resources
    - routes
    - tests

  excludePaths:
    - app/Http/Controllers/Auth/*
    - tests/Feature/Auth/*
    - app/Http/Requests/Auth/LoginRequest.php

  checkMissingIterableValueType: false
  checkModelProperties: true
  checkOctaneCompatibility: true



